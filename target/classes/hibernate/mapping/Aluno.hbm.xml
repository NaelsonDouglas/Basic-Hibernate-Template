<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   
   <class table="Aluno" lazy="false" name="pessoal.Aluno">
     
      <id name="matricula" type="int" column="matricula">
         <generator class="increment"/>
      </id>


	
	<property name="nome" type="string" column="nome"/>
	<property name="credsCumpridos" type="int" column="credsCumpridos"/>
	<property name="posGraduando" type="boolean" column="posGraduando"/>
	<property name="departamentoID" type="int" column="departamentoID"/>
	<property name="disciplinas" type="string" column="disciplinas_id"/>
	
	
	
	
	
	
	
		
	
	
	
	
	
	
	
      
      
      
   </class>
   
   
   
   
</hibernate-mapping>