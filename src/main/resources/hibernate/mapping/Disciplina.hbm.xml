<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   
   <class table="Disciplina" lazy="false" name="administrativo.Disciplina">
     
      <id name="bd_id" type="int" column="bd_id">
         <generator class="increment"/>
      </id>	
	
	<property name="discPreRequisitoID" type="string" column="discPreRequisitoID"/>
	<property name="nome" type="string" column="nome"/>
	<property name="id" type="string" column="id"/>
	<property name="minCreditos" type="int" column="minCreditos"/>
	<property name="obrigatoria" type="boolean" column="obrigatoria"/>
	<property name="oferecida" type="boolean" column="oferecida"/>
	
	
		
	
	
	
	
	
	
	
	
	
		
	
	
	
	
	
	
	
      
      
      
   </class>
   
   
   
   
</hibernate-mapping>